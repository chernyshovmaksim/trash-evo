
root = true

[*]
end_of_line = lf
insert_final_newline = true


# Indentation override for all JS under lib directory
[*.js]
indent_style = tab
indent_size = 2

[*.scss]
indent_style = tab
indent_size = 2

# Matches the exact files either package.json or .travis.yml
[{package.json,.travis.yml}]
indent_style = space
indent_size = 2
